<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.dl7.taglayout">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".TagShapeActivity">
        </activity>
        <activity android:name=".TagChoiceActivity">
        </activity>
        <activity android:name=".TagEditActivity">
        </activity>
        <activity android:name=".TagChangeActivity">
        </activity>
        <activity android:name=".TagViewActivity">
        </activity>
        <activity android:name=".TestActivity">
        </activity>
        <activity android:name=".TagReverseActivity">
        </activity>
    </application>

</manifest>